<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.phenlineaapp">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:fullBackupContent="@xml/backup_descriptor"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PHEnLineaAPP">
        <activity android:name=".MVP.Views.Activitys.PdfControlActivity"></activity>
        <activity android:name=".MVP.Views.Activitys.ControlEncuestasActivity" />
        <activity android:name=".MVP.Views.Activitys.ControlInformesActivity" />
        <activity android:name=".MVP.Views.Activitys.ControlBoletinesActivity" />
        <activity android:name=".MVP.Views.Activitys.ControlMensajesActivity" />
        <activity android:name=".MVP.Views.Activitys.ControlCertificadosActivity" />
        <activity android:name=".MVP.Views.Activitys.ControlFacturasActivity" />
        <activity android:name=".MVP.Views.Activitys.HomeActivity" />
        <activity android:name=".Base.Activitys.BaseActivity" />
        <activity android:name=".MVP.Views.Activitys.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>